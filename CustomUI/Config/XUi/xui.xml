<configs>
	<!-- Crafting slots -->
	<set xpath="/xui/ruleset[@name='default']/window_group[@name='crafting']/window[@name='windowCraftingQueue']/@name">PhDwindowCraftingQueue</set>
	<!-- Campfire crafting slots -->
	<set xpath="/xui/ruleset[@name='default']/window_group[@name='workstation_campfire']/window[@name='windowCraftingQueue']/@name">PhDwindowCraftingQueue</set>
	<!-- Forge crafting slots -->
	<set xpath="/xui/ruleset[@name='default']/window_group[@name='workstation_forge']/window[@name='windowCraftingQueue']/@name">PhDwindowCraftingQueue</set>
	<set xpath="/xui/ruleset[@name='default']/window_group[@name='workstation_forge']/window[@name='windowForgeInput']/@name">NNCwindowForgeInput</set>
	<!-- Cement mixer crafting slots -->
	<set xpath="/xui/ruleset[@name='default']/window_group[@name='workstation_cementMixer']/window[@name='windowCraftingQueue']/@name">PhDwindowCraftingQueue</set>
	<!-- Workbench crafting slots -->
	<set xpath="/xui/ruleset[@name='default']/window_group[@name='workstation_workbench']/window[@name='windowCraftingQueue']/@name">PhDwindowCraftingQueue</set>
	<!-- Chemistry station crafting slots -->
	<set xpath="/xui/ruleset[@name='default']/window_group[@name='workstation_chemistryStation']/window[@name='windowCraftingQueue']/@name">PhDwindowCraftingQueue</set>
	
<append xpath="/xui/ruleset[@name='default']">
		
		<window_group name="serverinfowindow">
			<window name="serverinfowindow" />
		</window_group>
		
		<window_group name="workstation_computer" controller="XUiC_WorkstationWindowGroup">
			<window name="windowCraftingList"/>
			<window name="craftingInfoPanel"/>
			<window name="PhDwindowCraftingQueue"/>
			<window name="windowOutput" />
			<window name="windowNonPagingHeader" />
		</window_group>	
		<window_group name="workstation_drinkingmixer" controller="XUiC_WorkstationWindowGroup">
			<window name="windowCraftingList"/>
			<window name="craftingInfoPanel"/>
			<window name="PhDwindowCraftingQueue"/>
			<window name="windowFuel" />
			<window name="windowOutput" />
			<window name="windowNonPagingHeader" />
		</window_group>
		<window_group name="workstation_cookingstove" controller="XUiC_WorkstationWindowGroup">
			<window name="windowCraftingList"/>
			<window name="craftingInfoPanel"/>
			<window name="PhDwindowCraftingQueue"/>
			<window name="windowToolsCampfire" />
			<window name="windowFuel" />
			<window name="windowOutput" />
			<window name="windowNonPagingHeader" />
		</window_group>
		<window_group name="workstation_furnace" controller="XUiC_WorkstationWindowGroup">
			<window name="windowCraftingList"/>
			<window name="craftingInfoPanel"/>
			<window name="PhDwindowCraftingQueue"/>
			<window name="windowToolsFurnace" />
			<window name="windowFuel" />
			<window name="windowFurnaceInput" />
			<window name="windowOutput" />
			<window name="windowNonPagingHeader" />
		</window_group>
		<window_group name="workstation_superforge" controller="XUiC_WorkstationWindowGroup">
			<window name="windowCraftingList"/>
			<window name="craftingInfoPanel"/>
			<window name="windowCraftingQueue"/>
			<window name="windowToolsSuperForge" />
			<window name="windowFuel" />
			<window name="NNCwindowForgeInput" />
			<window name="windowOutput" />
			<window name="windowNonPagingHeader" />
		</window_group>
</append>	
	
</configs>
