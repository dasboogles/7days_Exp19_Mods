<configs>
	
<append xpath="/blocks">

<block name="Love bed">
	<property name="DescriptionKey" value="bedGroupDesc"/>
	<property name="CustomIcon" value="bed02"/>
	<property name="Material" value="Mfurniture"/>
	<property name="Shape" value="Ext3dModel"/> <property name="Texture" value="293"/>
	<property name="Mesh" value="models"/>
	<property name="Model" value="Furniture/bed02" param1="main_mesh"/>
	<property name="DisplayType" value="blockMulti"/>
	<property name="MultiBlockDim" value="2,1,2"/>
	<property name="IsTerrainDecoration" value="true"/>
	<property name="FallDamage" value=".6"/>
	<!-- A15
	<property name="BuffsWhenWalkedOn" value="sleepingBigBedTrigger"/>
	-->
	<property class="RepairItems">
		<property name="resourceScrapIron" value="10"/>
	</property>
	<drop event="Destroy" name="resourceScrapIron" count="10"/>
	<property name="Group" value="Decor/Miscellaneous"/>
	<property name="EconomicValue" value="2600"/>
	<property name="FilterTags" value="fdecor,fother,ffurniture"/>
</block>

<block name="Victory Flag">
	<property name="CustomIcon" value="flagPoleWhiteRiver"/>
	<property name="DescriptionKey" value="EventDesc"/>
	<property name="CreativeMode" value="Player"/>
	<property name="DisplayType" value="blockHardenedMulti"/>
	<property name="Material" value="Mmetal"/>
	<property name="Shape" value="ModelEntity"/>
	<property name="Path" value="solid"/>
	<property name="Model" value="Entities/Decor/flag_polePrefab"/>
	<property name="MultiBlockDim" value="1,20,1"/>
	<property name="ImposterDontBlock" value="true"/>
	<property name="Place" value="TowardsPlacerInverted"/>
	<property name="LootList" value="42"/>
	<drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>
	<drop event="Harvest" name="ammo762mmBulletBall" count="10,15" tag="allHarvest"/>
	<drop event="Destroy" count="0"/>
	<property name="FilterTags" value="fdecor,fother"/>
</block>

